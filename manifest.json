{
  "name": "github-brief-description",
  "version": "1",
  "manifest_version": 2,
  "description": "Alert that looking at the old Github repository.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "css": ["style.css"],
      "js": ["jquery.min.js","script.js"],
      "run_at": "document_end"
    }
  ]
}
